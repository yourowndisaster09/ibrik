{
    "name": "ibrik",
    "description": "CoffeeScript coverage tool",
    "homepage": "http://github.com/Constellation/ibrik.html",
    "main": "./lib/ibrik",
    "directories": {
        "lib": "./lib",
        "tools": "./tools"
    },
    "bin": {
        "ibrik": "./bin/ibrik"
    },
    "version": "0.0.7",
    "engines": {
        "node": ">=0.4.0"
    },
    "maintainers": [{
        "name": "Yusuke Suzuki",
        "email": "utatane.tea@gmail.com",
        "web": "http://github.com/Constellation"
    }],
    "repository": {
        "type": "git",
        "url": "http://github.com/Constellation/ibrik.git"
    },
    "dependencies": {
        "coffee-script": "1.7.1",
        "istanbul": "*",
        "optimist": "*",
        "escodegen": "*",
        "mkdirp": "*",
        "which": "*",
        "estraverse": "*",
        "esprima": "1.0.x",
        "fileset": "0.1.x"
    },
    "devDependencies": {
        "mocha": "*",
        "chai": "*"
    },
    "licenses": [{
        "type": "BSD",
        "url": "http://github.com/Constellation/ibrik/raw/master/LICENSE.BSD"
    }],
    "scripts": {
        "test": "./node_modules/.bin/mocha",
        "install": "npm run-script build",
        "build": "make -f tools/task.mk build",
        "watch": "make -f tools/task.mk watch"
    }
}
